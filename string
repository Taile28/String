-- Define the IP address whitelist
local ipWhitelist = {
    ["173.46.253.5"] = true,
    ["10.0.0.1"] = true,
    -- Add more IP addresses as needed
}

-- Function to check if an IP address is whitelisted
local function isIPWhitelisted(ip)
    return ipWhitelist[ip] or false
end

return isIPWhitelisted
